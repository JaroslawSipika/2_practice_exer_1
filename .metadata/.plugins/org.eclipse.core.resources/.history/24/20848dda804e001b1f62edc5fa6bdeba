
public class ZagadkowyBasen1 {

	public static void main(String[] args) {
		int x = 0;
		
		while (x < 4) {
			x = x + 1;
			if (x < 1) {
				System.out.print("a ");
			}
			x = x + 2;
			if (x < 4) {
				System.out.print("n ");
				x = x -2;
			}
			if (x == 1) {
				System.out.print("an");
			}
			if (x > 3) {
				System.out.print("talek");
			}
			System.out.println("");
			x = x - 1;
		}

	}

}
