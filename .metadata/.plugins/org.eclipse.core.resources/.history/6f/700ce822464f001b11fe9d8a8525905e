
class CPole {
	
	int obliczPole(int wysokosc, int szerokosc) {
		return wysokosc * szerokosc;
	}
	
	
}





public class Pole {

	public static void main(String[] args) {
		
		
		CPole pol = new CPole();
		
		
		int a = pol.obliczPole(7,12);
		short c = 7;
		int b = pol.obliczPole(c, 15);
		
		
		
		System.out.println(a);
		System.out.println(b);

	}

}
